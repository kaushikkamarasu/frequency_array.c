#include <stdio.h>

int main() {
    int arr[1000];
    int i,n,count=1;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
   
    for(i=0;i<n;i++){
        if(arr[i]==arr[i+1]){
            count++;
        }
        else{
            printf("the frequency of %d is %d \n",arr[i],count);
        }
    }
	return 0;
}
